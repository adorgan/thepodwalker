<%- include ('partials/header') %>

<div class="container d-flex justify-content-center">
    
    <div class="row">
        <div class="col-12  pt-3">
            
        
        <!-- <form autocomplete=off action="/subscribe" method="POST"> -->
        <form id="form_subscribe" autocomplete=off>
            <div class="form-group">
                <label>First Name</label>
                <input type="text" class="form-control" id="subscribe_first_name" name="email[firstName]">
                <label>Last Name</label>
                <input type="text" class="form-control" id="subscribe_last_name" name="email[lastName]">
              </div>
            <div class="form-group">
                <label>Email</label>
                <input type="email" class="form-control" id="subscribe_email" name="email[email]">
            </div>
            <div class="d-flex justify-content-center">
            <input id="subscribe_button" class="btn btn-primary " type="submit">
        </div>
        </form>
        <div id="subscribe_success" class="subscribe-status p-2">
            
        </div>
    </div>
    
    <br>
    

</div>


